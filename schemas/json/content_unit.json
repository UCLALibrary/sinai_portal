{
  "title": "Content Unit",
  "description": "A Content Unit defines a piece or pieces of content presented in a given manuscript in a unified form; may contain one or more work witnesses (REVISE)",
  "type": "object",
  "properties": {
    "id": {
      "description": "A unique identifer, supplied by the database",
      "type": "integer"
    },
    "ark": {
      "description": "A unique Archival Resource Key (ARK) describing the manuscript object",
      "type": "string",
      "pattern": "^ark:/21198/z1.{6}"
    },
    "cod_units": {
      "description": "The database identifiers for the codicological units that reference this content unit record",
      "type": "array",
      "items": {
        "description": "A unique identifer, supplied by the database",
        "type": "integer"
      },
      "minItems": 1,
      "unevaluatedItems": false
    },
    "type": {
      "description": "The type of manuscript object, whether real or hypothetical reconstructions",
      "enum": [
        "shelf",
        "rebind"
      ],
      "$comment": "Is this the same set as #/$defs/ms_obj_types?"
    },
    "summary": {
      "type": [
        "string",
        "null"
      ],
      "minLength": 1
    },
    "locus": {
      "type": "string",
      "minLength": 1,
      "$comment": "Can use format to restrain to regex for, e.g., 'ff. x-y'"
    },
    "lang": {
      "type": "array",
      "items": {
        "description": "Names of the ancient languages used in Sinai Manuscripts Data Portal",
        "enum": [
          "Arabic",
          "Armenian",
          "Caucasian Albanian",
          "Church Slavonic",
          "Coptic",
          "CPA",
          "Ethiopic",
          "Georgian",
          "Greek",
          "Latin",
          "Syriac",
          "Turkish"
        ]
      },
      "minItems": 1,
      "unevaluatedItems": false
    },
    "work_wit": {
